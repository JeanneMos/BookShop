    "@testing-library/user-event": "^14.0.4",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "blob-polyfill": "^6.0.20211015",
    "css-loader": "^4.3.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.14.0",
