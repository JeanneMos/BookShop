    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.26.0",
    "classnames": "^2.3.1",
    "compressorjs": "^1.1.1",
    "dotenv": "^16.0.0",
    "focus-trap-react": "^8.9.2",
    "html-entities": "^2.3.3",
    "js-cookie": "^3.0.1",
    "nanoid": "^3.3.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.16",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.2.1",
