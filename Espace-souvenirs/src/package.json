{
  "name": "espace-souvenir",
  "version": "1.0.0",
  "description": "Espace Souvenirs",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean_start": "rimraf dist_dev",
    "test": "jest test --coverage=false",
    "test:build": "jest test",
    "start": "npm run clean_start && npm run postcss && webpack --watch",
    "build": "npm run clean && webpack --env production",
    "pretty-quick": "pretty-quick",
    "postcss": "postcss --no-map --replace docroot/themes/custom/souvenirs/src/**/*scss",
    "prepare": "cd .. && husky install .husky"
  },
  "repository": {
    "type": "git",
    "url": "https://internet-drupal-ogf.visualstudio.com/SITE%20INTERNET%20%20DRUPAL/_git/espace-souvenir"
  },
  "author": "OGF",
  "license": "ISC",
